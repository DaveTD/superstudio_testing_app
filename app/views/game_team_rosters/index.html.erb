<p id="notice"><%= notice %></p>

<h1>Game Team Rosters</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_team_rosters.each do |game_team_roster| %>
      <tr>
        <td><%= link_to 'Show', game_team_roster %></td>
        <td><%= link_to 'Edit', edit_game_team_roster_path(game_team_roster) %></td>
        <td><%= link_to 'Destroy', game_team_roster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game Team Roster', new_game_team_roster_path %>
